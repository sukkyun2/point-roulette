{
  "name": "point-roulette-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:user": "pnpm --filter user-app dev",
    "dev:admin": "pnpm --filter admin-app dev",
    "build": "pnpm -r build",
    "build:user": "pnpm --filter user-app build",
    "build:admin": "pnpm --filter admin-app build",
    "lint": "pnpm -r lint",
    "type-check": "pnpm -r type-check",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "pnpm -r clean",
    "dev:openapi": "curl -o openapi.yaml http://localhost:8080/v3/api-docs.yaml && pnpm orval"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.13",
    "eslint": "^9.39.1",
    "globals": "^16.5.0",
    "orval": "^7.21.0",
    "prettier": "^3.8.1",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.48.0"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.21",
    "axios": "^1.13.5",
    "vite": "^7.3.1"
  }
}
